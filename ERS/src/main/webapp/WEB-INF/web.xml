<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
	<display-name>Archetype Created Web Application</display-name>
  	<servlet>
		<servlet-name>FrontEndServlet</servlet-name>
		<display-name>FrontEndServlet</display-name>
		<description></description>
		<servlet-class>servlet.FrontEndServlet</servlet-class>
	</servlet>
	<servlet-mapping>
  		<servlet-name>FrontEndServlet</servlet-name>
  		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<welcome-file-list>  
		<welcome-file>/HTMl/startPage.html</welcome-file>  
	</welcome-file-list>
	<error-page>
  		<error-code>400</error-code>
  		<location>/startPage.html</location>
  	</error-page>
  	<error-page>
  		<exception-type>java.lang.Throwable</exception-type>
  		<location>/startPage.html</location>
  	</error-page>   
</web-app>
